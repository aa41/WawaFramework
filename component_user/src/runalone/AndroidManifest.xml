<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.duiba.component_user">
    <!--WindowManager实现悬浮窗权限-->
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />
    <application
        android:name=".application.UserApplication"
        android:allowBackup="false"
        android:label="@string/user_app_name"
        android:supportsRtl="true"
        android:theme="@style/base_commonAppTheme"
        tools:replace="android:label">

        <activity android:name=".UserTestActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".home.view.UserHomeActivity" />
        <activity android:name=".home.view.UserOtherActivity" />
    </application>

</manifest>
